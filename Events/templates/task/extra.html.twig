{% extends 'base.html.twig' %}

{% block title %}Horas extra{% endblock %}



{% block body %}


{% for task in tasks %}
<form action="{{ path('app_task_edit_extra', {'id': task.id})}}">
    {{task.name}} Horas Extra: {{task.getExtraTime}}
    <input type="number" name="extra_time" min="0" max="10">
	<input type="submit" value="Modificar Hora Extra">
</form>

{% endfor %}



{% endblock %}